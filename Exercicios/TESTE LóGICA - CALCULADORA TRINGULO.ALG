Algoritmo "Teste Lógica - calculadora tringulo"


Var
    altura, base, area: real


Inicio

      escreva ("Insira a altura = ")
      leia (altura)
      escreva ("Insira a base = ")
      leia (base)
      
      area <- (base*altura) /2
      
      escreval ("A area do tringulo é = ", area)


Fimalgoritmo